//SAKHIL N MAJU - 56
#include<stdio.h>
void main()
{
    int n,arr[100],i,x,flag=0;
    printf("ENTER LIMIT : ");
    scanf("%d",&n);
    printf("ENTER ARRAY ELEMENTS : \n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("ENTER THE ELEMENT TO SEARCH : ");
    scanf("%d",&x);
    for(i=0;i<n;i++)
    {
        if(arr[i]==x)
        {
            printf("ELEMENT FOUND AT %d POSITION",i+1);
            flag=1;
            break;
        }
       
    }
    if(flag==0)
    {
        printf("ELEMENT NOT FOUND");
    }
}    

import java.util.*;
class Demoprime
{
	int prime(int a)
	{
		for(int i=2;i<a;i++)
		{	
			if((a%i) == 0)
			{
				return(a);
				break;
			}
		}
	}	
}
class n_prime
{
	public static void main(String args[])
	{
		Scanner s = new Scanner(System.in);
		Demoprime ob = new Demoprime();
		System.out.print("ENTER NUMBER : ");
		int a = s.nextInt();
		System.out.println("THE PRIME NUMBERS ARE :");
		for(int i=2;i<a;i++)
		{
			int p = ob.prime(i);
			System.out.println(p);
		}
				
	}
}
